algoritmo "18"
var
   numero: inteiro
   i: inteiro
inicio
   para i de 1 ate 5 faca
      escreva("Digite um número entre 65 e 119: ")
      leia(numero)

      se (numero >= 65) e (numero <= 119) entao
         escreval("Caractere correspondente: ", carac(numero))
      senao
         escreval("Número fora do intervalo!")
      fimse
   fimpara
fimalgoritmo
















